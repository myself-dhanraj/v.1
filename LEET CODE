PROBLEM 136: LEET CODE

Single Number

class Solution {
public:
    int singleNumber(vector<int>& nums) {

        int ans = 0;

        for(int val : nums){
            ans = ans^val;
        }
        return ans;
    }
};

Problem 53 : max subarry using Kadane's Algorithm

class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int currSum = 0, maxSum = INT_MIN;

        for(int val : nums){
            currSum+= val;
            maxSum = max(currSum , maxSum);

            if (currSum<0){
                currSum = 0;
            }
        }

         return maxSum;
    }
};
